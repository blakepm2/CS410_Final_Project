{
  "manifest_version": 3,
  "name": "Historian",
  "description": "Build a knowledge graph of your history to find ",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "history",
    "http://127.0.0.1/*"
  ],
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}